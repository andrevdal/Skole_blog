{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"outDir": "./dist/",
		"rootDir": "./src/",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"resolveJsonModule": true,
	"exclude": ["./node_modules/", "./dist/", "./src/public/**/*"],
	"include": ["./src/**/*"]
}
