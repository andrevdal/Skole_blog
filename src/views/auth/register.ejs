<html lang="en">

	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Home</title>
		<link rel="stylesheet" href="/cdn/stylesheets/register.css">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
	</head>

	<body>
		<div class="logo">
			<img src="/cdn/assets/assbook.svg" />
		</div>
		<div class="loginButtonMain">
			Login
		</div>
		<div class="loginBox">
			<div class="loginBorder">
				<div class="x">
					<img src="/cdn/assets/x.png" alt="exit">
				</div>
				<div class="instructions">
					Register new account
				</div>
				<form class="login">
					<input type="text" placeholder="Username" class="username">
					<input type="password" placeholder="Password" class="password">
					<input type="password" placeholder="Retype password" class="retypePassword">
					<p class="loginFeedback">Enter username and password</p>
					<div class="loginButton">
						<button type="submit">Register new account</button>
					</div>
					<a href="/auth/login?login=true" class="register">Already have an account? Login</a>
				</form>
			</div>
		</div>
		<script type="module" src="/cdn/scripts/auth/register.js"></script>
	</body>
</html>
